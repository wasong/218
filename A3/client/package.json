    "rules": {
      "semi": [
        2,
        "never"
      ],
      "react/jsx-filename-extension": "off",
      "max-len": "off"
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack.config.js"
        }
      }
    }
  }
